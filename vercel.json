{
  "functions": {
    "api/compile.ts": {
      "memory": 1024,
      "maxDuration": 10,
      "includeFiles": "bin/**"
    }
  },
  "rewrites": [
    {
      "source": "/api/lua/:path*",
      "destination": "https://api.luaobfuscator.com/v5/obfuscator/:path*"
    },
    {
      "source": "/api/compile",
      "destination": "/api/compile"
    },
    {
      "source": "/((?!api/.*).*)",
      "destination": "/index.html"
    }
  ]
}